mixin floor()
    section#floor.section.floor
        .floor__body.container 
            h2.floor__title Floor plans
            .floor__tab
                button.floor__tab-item.active(type='button' data-target='#floor1') 1 bedroom
                button.floor__tab-item(type='button' data-target='#floor2') 2 bedrooms
                button.floor__tab-item(type='button' data-target='#floor3') 3 bedrooms
